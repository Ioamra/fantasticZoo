<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="350.0" style="-fx-background-color: #333333;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.modals.CleanEnclosureModalController">
    <children>
        <Text fill="WHITE" layoutY="27.0" text="Nettoyer Enclos" textAlignment="CENTER" wrappingWidth="350.0">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Text>
        <Button layoutY="100.0" mnemonicParsing="false" onAction="#confirmCleaning" text="Confirmer" textFill="BLACK" AnchorPane.leftAnchor="70.0" AnchorPane.rightAnchor="200.0" />
        <Button layoutY="100.0" mnemonicParsing="false" onAction="#cancelCleaning" text="Annuler" textFill="BLACK" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="70.0" />
    </children>
</AnchorPane>
